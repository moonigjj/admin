<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.md.admin.mapper.SysRoleMapper">
    <select id="findByName" resultType="com.md.admin.entity.SysRole">
        select *
        from sys_role
        where role_name = #{name}
        limit 1
    </select>

</mapper>
